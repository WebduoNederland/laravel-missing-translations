{
    "name": "webduonederland/laravel-missing-translations",
    "description": "Laravel package to detect and store missing translations in a database table",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Finn Paes",
            "email": "finn@webduo.nl"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "WebduoNederland\\LaravelMissingTranslations\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "WebduoNederland\\LaravelMissingTranslations\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "style": "./vendor/bin/pint",
        "analyse": "./vendor/bin/phpstan analyse --memory-limit=2G"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "larastan/larastan": "^3.7",
        "laravel/pint": "^1.25",
        "orchestra/testbench": "^10.6"
    }
}
